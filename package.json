{
  "name": "isotolanguage",
  "version": "1.0.0",
  "description": "light and fast module to convert iso country and languages codes into country names, there is no unnecessary stuff only the bare essentials",
  "repository": "https://github.com/erikgolinelli/isotolanguage.git",
  "scripts": {
    "build": "webpack --mode production",
    "postbuild": "tsc -p tsconfig.json --emitDeclarationOnly --declaration --declarationDir ./lib/@types --skipLibCheck",
    "dev": "webpack --mode development --watch",
    "lint": "eslint ./src --fix",
    "test": "jest --coverage",
    "prepare": "husky install"
  },
  "main": "lib/index.js",
  "keywords": [
    "iso",
    "country",
    "code",
    "language"
  ],
  "author": "Erik Golinelli <erik@codekraft.it>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "better-docs": "^2.7.3",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "lint-staged": "^15.2.0",
    "prettier": "^2.8.8",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
