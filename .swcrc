{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true
    },
    "target": "esnext",
    "externalHelpers": true,
    "minify": {
      "compress": true,
      "mangle": true
    }
  },
  "sourceMaps": true
}
